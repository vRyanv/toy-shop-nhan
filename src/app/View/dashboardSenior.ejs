<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="icon" href="/image/icon-dashboard.png">
    <title>Dashboard</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
          rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
          crossorigin="anonymous">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="/css/animation.css">
    <link rel="stylesheet" href="/css/dashboard.css">
    <link rel="stylesheet" href="/css/navbar.css">
    <link rel="stylesheet" href="/css/product.css">
</head>
<body>
<div style="position: fixed; width: 100%; top: 0; z-index: 100">
    <ul class="menu-bar">
            <li>ATN.COM</li>
        <% if(role === '2') {%>
            <a href="/dashboard-senior"><li <% if(module === 'shop') {%> class="current" <%}%> >Shop</li></a>
            <a href="/staff"><li <% if(module === 'staff') {%> class="current" <%}%>  >Staff</li></a>
        <% }else {%>
            <a href="/dashboard-admin"><li <% if(module === 'product') {%> class="current" <%}%>  >Product</li></a>
            <a href="/category"><li <% if(module === 'category') {%> class="current" <%}%>  >Category</li></a>
            <a href="/supplier"><li <% if(module === 'supplier') {%> class="current" <%}%>  >Supplier</li></a>
        <% }%>
            <a href="/logout"><li>Logout</li></a>
    </ul>
</div>
<div id="root" style="padding-top: 5rem;">
    <!--animation-->
    <%- include('animation.ejs') %>
    <!--animation-->
    <% if(module === 'shop') {%>
        <%- include('shop.ejs') %>
    <% } else if(module === 'staff'){%>
        <%- include('staff.ejs') %>
    <% } else if(module === 'product'){%>
        <%- include('product.ejs') %>
    <% } else if(module === 'category'){%>
        <%- include('category.ejs') %>
    <% } else if(module === 'supplier'){%>
        <%- include('supplier.ejs') %>
    <% } %>
</div>
<!-- Modal -->
<div class="modal fade" id="confirm_delete" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h1 class="modal-title fs-5" id="exampleModalLabel">Confirm</h1>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body" id="modal_body_delete">

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                <button type="button" id="btn_ok_delete" class="btn btn-primary">Ok</button>
            </div>
        </div>
    </div>
</div>
</body>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js"
        integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
<% if(module === 'shop') {%>
    <script src="/js/shop.js"></script>
<% } else if(module === 'staff'){%>
    <script src="/js/staff.js"></script>
<% } else if(module === 'category'){%>
    <script src="/js/category.js"></script>
<% } else if(module === 'product'){%>
    <script src="/js/product.js"></script>
<% } else if(module === 'supplier'){%>
    <script src="/js/supplier.js"></script>
<% } %>


</html>